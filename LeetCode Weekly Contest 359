class Solution {
public:
    bool isAcronym(vector<string>& w, string s) {
        
        if(s.size()!=w.size()){
            return false;
        }
        
        bool flag = true;
        
        for(int i =0;i<s.size();i++){
            string temp = w[i];
            
            if(temp[0]!=s[i]){
                return false;
            }
            
        }
        
        return true;
        
    }
};
